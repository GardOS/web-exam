{
  "name": "web-exam",
  "version": "1.0.0",
  "main": "index.jsx",
  "scripts": {
    "start": "npm run build && node src/server/server.js",
    "build": "webpack --mode production",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "webpack-dev-server --mode development --hot",
    "server": "npm run db && nodemon src/server/server.js",
