{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "comma-dangle": "off",
    "react/destructuring-assignment": ["never"],
    "no-console": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "linebreak-style": ["error", "windows"]
  }
}
